{
  "name": "event-sourcing-nestjs",
  "version": "1.2.11",
  "description": "NestJS module for implementing Event Sourcing.",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix && npm run prettier",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "deps-update": "npx npm-check-updates --packageFile package.json -i"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@types/node": "^14.14.6",
    "dot-prop": "^6.0.0",
    "elasticsearch": "^16.7.1",
    "eventstore": "^1.15.5",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.2",
    "redis": "^3.0.2",
    "short-uuid": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/common": "7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/platform-express": "^7.5.1",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spunkypanda/event-store-nestjs.git"
  },
  "keywords": [
    "nestjs",
    "es",
    "eventsourcing",
    "cqrs",
    "ddd"
  ],
  "author": "spunkypanda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spunkypanda/event-store-nestjs/issues"
  },
  "homepage": "https://github.com/spunkypanda/event-store-nestjs#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "directories": {
    "doc": "docs"
  }
}
