{
  "name": "event-sourcing-nestjs",
  "version": "1.2.10",
  "description": "NestJS module for implementing Event Sourcing.",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix && npm run prettier",
    "prettier": "prettier --write 'src/*.{ts, js}'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@types/node": "^13.5.0",
    "@types/oracledb": "^5.0.0",
    "dot-prop": "^5.2.0",
    "elasticsearch": "^16.7.1",
    "eventstore": "^1.3.1",
    "lodash": "^4.17.20",
    "mongodb": "^3.5.9",
    "oracledb": "^5.0.0",
    "redis": "^3.0.2",
    "short-uuid": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/platform-express": "^7.6.18",
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "jest": "^26.4.2",
    "prettier": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spunkypanda/event-store-nestjs.git"
  },
  "keywords": [
    "nestjs",
    "es",
    "eventsourcing",
    "cqrs",
    "ddd"
  ],
  "author": "spunkypanda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spunkypanda/event-store-nestjs/issues"
  },
  "homepage": "https://github.com/spunkypanda/event-store-nestjs#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "directories": {
    "doc": "docs"
  }
}
